{
  "name": "rawmodel",
  "version": "1.3.0",
  "description": "Strongly-typed JavaScript object with support for validation and error handling.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 4,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean; tsc",
    "example": "tsc -p example/tsconfig.json && node example/dist",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublish": "npm run build",
    "test": "npm run build && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/rawmodeljs.git"
  },
  "bugs": {
    "url": "https://github.com/xpepermint/rawmodeljs/issues"
  },
  "homepage": "https://github.com/xpepermint/rawmodeljs#readme",
  "keywords": [
    "model",
    "modeling",
    "document",
    "object",
    "schema",
    "orm",
    "odm",
    "map",
    "mapping",
    "context",
    "contextify",
    "rawmodel",
    "class",
    "db",
    "database",
    "storage",
    "structure",
    "structuring",
    "error",
    "errors",
    "handling",
    "handle",
    "valid",
    "validate",
    "validation",
    "validator",
    "validating",
    "type",
    "cast",
    "casting",
    "history",
    "tracking",
    "change",
    "field",
    "fields"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "tslint": "4.3.1",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "handleable": "^0.19.0",
    "lodash.isequal": "^4.4.0",
    "lodash.merge": "^4.6.0",
    "typeable": "^2.0.0",
    "validatable": "^0.24.0"
  }
}
